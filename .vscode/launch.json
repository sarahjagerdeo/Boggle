    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) spellcheck",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/spellcheck",
            "args": ["${workspaceFolder}/dictionary.txt"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        },
        
        {
            "name": "(gdb) oneplayer 100",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/onePlayerBoggle",
            "args": ["${workspaceFolder}/dictionary.txt", "100"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        },

        {
            "name": "(gdb) oneplayer 200",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/onePlayerBoggle",
            "args": ["dictionary.txt", "100"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        }
        
    ]
}